{{#each this}}
      <div>
        <h2 class="name-countries">{{name}}</h2>
        <p class="capital">Capital:{{capital}}</p>
        <p class="population">Population:{{population}}</p>
        <p class="languages">  </p>
          Languages:
          <ul>
          {{#each languages}}
            <li class ="list"> {{this.name}}</li>
          {{/each}}
          </ul>
        </p>
      </div>
      <img class="flag-img" src="{{flag}}" width="300" height="200" alt="{{name}} flag">
    </div>
{{/each}}
